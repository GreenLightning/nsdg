<?xml version="1.0" ?>

<diagram title="Test" xmlns="http://greenlightning.eu/nsdg"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://greenlightning.eu/nsdg nsdg.xml">
	<branch condition="x == 0">
		<left label="Yes">
			<block>Block</block>
		</left>
		<right label="No">
			<procedure>Procedure</procedure>
			<break>Break</break>
		</right>
	</branch>
	<loop type="infinite" top="infinite loop" side="do" bottom="forever">
		<block>X</block>
	</loop>
	<loop condition="for int i = 0; i \l 5; i++" side="do">
		<switch expression="i">
			<case label="1">
				<block>Case 1 is really long.</block>
			</case>
			<case label="2" />
			<case label="3" />
			<default label="default" />
		</switch>
	</loop>
	<switch expression="test">
		<case label="0">
			<block>test</block>
		</case>
	</switch>
</diagram>