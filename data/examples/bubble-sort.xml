<?xml version="1.0" ?>

<diagram title="Bubble Sort" xmlns="http://greenlightning.eu/nsdg"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://greenlightning.eu/nsdg ../nsdg.xml">
	<loop condition="for i from n to 0">
		<loop condition="for j from 1 to i">
			<branch condition="a[j - 1] \l a[j]">
				<left label="yes">
					<block>
						h = a[j - 1]
						a[j - 1] = a[j]
						a[j] = h
					</block>
				</left>
				<right label="no" />
			</branch>
		</loop>
	</loop>
</diagram>