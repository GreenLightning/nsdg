	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://greenlightning.eu/nsdg ../nsdg.xml">
	<loop condition="fÃ¼r i = 1 bis n - 1, i = i + 1">
		<block>
			wert = a[i]
			j = i
		</block>
		<loop condition="solange (j \g 0) und (a[j - 1] \g wert)">
